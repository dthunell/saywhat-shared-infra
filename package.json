{
  "name": "saywhat-bootstrap",
  "version": "1.0.0",
  "description": "",
  "main": "dist/app.js",
  "scripts": {
    "build": "npm run compile && npm run lint",
    "compile": "./node_modules/.bin/tsc --build tsconfig.json",
    "lint": "eslint \"**/*.ts\" --quiet --fix",
    "start": "node dist/api.js",
    "test": "jest --coverage --verbose",
    "docker-test": "docker-compose up --build --abort-on-container-exit"
  },
  "author": "Daniel Thunell",
  "license": "ISC",
  "jest": {
    "collectCoverageFrom": [
      "**/*.ts",
      "!**/console.js",
      "!**/node_modules/**",
      "!**/build/**",
      "!**/migrations/**",
      "!**/config/**",
      "!**/scripts/**",
      "!**/docs/**"
    ],
    "testEnvironment": "node",
    "testRegex": "(/tests/.*|(\\.|/)(test|spec))\\.[jt]sx?$",
    "transform": {
      "^.+\\.ts?$": "ts-jest"
    }
  },
  "dependencies": {
    "axios": "^0.20.0",
    "lambda-api": "^0.10.6"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.64",
    "@typescript-eslint/eslint-plugin": "^4.5.0",
    "@typescript-eslint/eslint-plugin-tslint": "^4.5.0",
    "@typescript-eslint/parser": "^4.5.0",
    "aws-sdk": "^2.775.0",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-prettier": "^3.1.2",
    "jest": "^26.6.0",
    "nock": "^13.0.4",
    "prettier": "^2.1.2",
    "prettier-eslint": "^11.0.0",
    "ts-jest": "^26.4.1",
    "typescript": "^4.0.3"
  }
}
